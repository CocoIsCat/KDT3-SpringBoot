<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.adminpage.mapper.ContactMapper">

    <select id="cntContact" resultType="int">
        select
            count(*)
        from
            contact
    </select>

    <select id="selectContact" resultType="com.example.adminpage.dto.Contact">
        select
            *
        from
            contact
    </select>
</mapper>