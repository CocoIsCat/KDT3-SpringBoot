<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.adminpage.mapper.MemberMapper">

    <select id="memberCount" resultType="int">
        select
            count(*)
        from
            Member
    </select>

    <select id="memberList" resultType="com.example.adminpage.dto.Member">
        select
            *
        from
            Member
    </select>
</mapper>